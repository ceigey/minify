<IfModule mod_rewrite.c>
RewriteEngine on

# You may need RewriteBase on some servers
RewriteBase /minify

# rewrite URLs like "/min/f=..." to "/min/?f=..."
RewriteCond  %{REQUEST_URI}  !index.php$
RewriteRule ^(.*)  index.php?g=$1&%{QUERY_STRING}  [L]
</IfModule>
